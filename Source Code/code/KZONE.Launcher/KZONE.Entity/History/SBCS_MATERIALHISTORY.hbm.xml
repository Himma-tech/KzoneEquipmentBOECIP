		</property>
		<property name="EVENT" type="String">
			<column name="EVENT" length="50" sql-type="varchar" not-null="false"/>
		</property>
		<property name="OLDMATERIALID" type="String">
			<column name="OLDMATERIALID" length="50" sql-type="varchar" not-null="false"/>
